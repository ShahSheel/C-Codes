#include<stdio.h>
 
void DFS(int);
int G[10][10],visited[10],n;    
 
void main()
{
    int i,j;
    printf("Number of vertices:");
    scanf("%d",&n);
    printf("Enter adjecency matrix of the graph:\n");
   
    for(i=0;i<n;i++)
       for(j=0;j<n;j++)
            scanf("%d",&G[i][j]);
 
   for(i=0;i<n;i++)
        visited[i]=0; 
    DFS(0);
}
 
void DFS(int x)
{
    int j;
    printf("\n%d",i);
    visited[x]=1;
    
    for(j=0;j<n;j++)
       if(!visited[j]&&G[x][j]==1)
            DFS(j);
}
